#!/bin/sh

cd "$(dirname "$0")/../incanter" &&
if test ! -d .git
then
	echo "The 'incanter' submodule was not checked out!"
	exit 1
fi &&
../bin/leiningen deps &&
cd lib &&
for lib in *.jar
do
	case "$lib" in
	clojure-1*|itext*|jcommon*|jfreechart*|junit*) ;;
	*)
		cp "$lib" ../../jars/
		;;
	esac
done
